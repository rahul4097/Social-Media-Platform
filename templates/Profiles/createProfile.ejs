<div class="col py-3">
    <div class="row">
        <div class="col">
            <nav aria-label="breadcrumb">
                <ol class="breadcrumb">
                    <li class="breadcrumb-item"><a href="/profile">Dashboard</a></li>
                    <li class="breadcrumb-item active"><%= //user.userName %></li>
                </ol>
            </nav>
        </div>
  
        <div class="col text-end fw-lighter">
            <b>Last Updated <%= //new Date(user.updatedAt).toUTCString() %></b><br>
            <b>UserId:</b> <%= //user._id %>
        </div>
    </div>
  </div>
  
  
  <form class="row g-3" action="/profile/createProfile" method="post" >
    <div class="col-md-6">
     <label for="inputEmail4" class="form-label">User Name</label>
     <input type="text" class="form-control" id="inputEmail4" name="userName">
   </div>
    <div class="col-md-6">
     <label for="inputEmail4" class="form-label">first Name</label>
     <input type="text" class="form-control" id="inputEmail4" name="fname">
   </div>
   <div class="col-md-6">
     <label for="inputEmail4" class="form-label">Last Name</label>
     <input type="text" class="form-control" id="inputEmail4" name="lname">
   </div>
   <div class="col-md-6">
     <label for="inputPassword4" class="form-label">About</label>
     <input type="text" class="form-control" id="inputPassword4" name="about">
   </div>
    <div class="col-md-6">
     <label for="inputPassword4" class="form-label">Intrest</label>
     <input type="text" class="form-control" id="inputPassword4" name="Intrest">
   </div>
   <div class="col-md-6">
     <label for="inputCity" class="form-label">Country</label>
     <input type="text" class="form-control" id="inputCity" name="country">
   </div>
   <div class="col-12">
     <div class="form-check">
       <input class="form-check-input" type="checkbox" id="gridCheck">
       <label class="form-check-label" for="gridCheck">
         Check me out
       </label>
     </div>
   </div>
   <div class="col-12">
     <button type="submit" class="btn btn-primary">Submit Your Profile</button>
   </div>
  </form>
  